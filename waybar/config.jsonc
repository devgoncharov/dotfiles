{
    "layer": "top", // Waybar at top layer
    // "position": "bottom", // Waybar position (top|bottom|left|right)
    "height": 30, // Waybar height (to be removed for auto height)
    // "width": 1280, // Waybar width
    "spacing": 4, // Gaps between modules (4px)
    // Choose the order of the modules
    "modules-left": ["hyprland/workspaces", "network"],
    "modules-center": ["clock", "custom/clock"],
    "modules-right": ["pulseaudio", "cpu", "memory", "tray"],
    "keyboard-state": {
        "numlock": true,
        "capslock": true,
        "format": "{name} {icon}",
        "format-icons": {
            "locked": "ÔÄ£",
            "unlocked": "ÔÇú"
        }
    },
    "sway/mode": {
        "format": "<span style=\"italic\">{}</span>"
    },
    "sway/scratchpad": {
        "format": "{icon} {count}",
        "show-empty": false,
        "format-icons": ["", "Ôãí"],
        "tooltip": true,
        "tooltip-format": "{app}: {title}"
    },
    "idle_inhibitor": {
        "format": "{icon}",
        "format-icons": {
            "activated": "ÔÅÆ",
            "deactivated": "ÔÅ∞"
        }
    },
    "tray": {
        "spacing": 10
    },
    "clock": {
        "tooltip-format": "<big>{:%d/%m/%Y}</big>\n<tt><small>{calendar}</small></tt>",
        "format": "{:%d/%m/%Y}"
    },
    "cpu": {
        "format": "{usage}% Ôãõ",
        "tooltip": false
    },
    "memory": {
        "format": "{}% ÔÉâ"
    },
    "network": {
        // "interface": "wlp2*", // (Optional) To force the use of this interface
        "format-wifi": "{essid} ({signalStrength}%) Ôá´",
        "format-ethernet": "{ipaddr} Ôõø ",
        "tooltip-format": "{ifname} via {gwaddr} Ôûñ",
        "format-linked": "{ifname} (No IP) Ôûñ",
        "format-disconnected": "Disconnected ‚ö†",
        "format-alt": "{ifname}: {ipaddr}/{cidr}"
    },
    "pulseaudio": {
        "scroll-step": 5, // %, can be a float
        "format": "{volume}% {icon}",
        "format-bluetooth": "{volume}% {icon}Ôäî {format_source}",
        "format-bluetooth-muted": "Ôö© {icon}Ôäî {format_source}",
        "format-muted": "Ôö© {format_source}",
        "format-source": "",
        "format-source-muted": "ÔÑ±",
        "format-icons": {
            "headphone": "ÔÄ•",
            "hands-free": "Ôñê",
            "headset": "Ôñê",
            "phone": "ÔÇï",
            "portable": "ÔÇï",
            "car": "ÔÜπ",
            "default": ["ÔÄ¶", "ÔÄß", "ÔÄ®"]
        },
        "on-click": "pavucontrol"
    },
    "custom/media": {
        "format": "{icon} {}",
        "return-type": "json",
        "max-length": 40,
        "format-icons": {
            "spotify": "ÔÜº",
            "default": "üéú"
        },
        "escape": true,
        "exec": "$HOME/.config/waybar/mediaplayer.py 2> /dev/null" // Script in resources folder
        // "exec": "$HOME/.config/waybar/mediaplayer.py --player spotify 2> /dev/null" // Filter player based on name
    },
    "hyprland/workspaces": {
     	"disable-scroll": true,
	"on-click": "activate",
	"all-outputs": true,
	"persistent-workspaces": {
		".": ".",
		"2": "",
		"3": "",
		"4": "",
		"5": "",
		"6": "",
		"7": "",
		"8": "",
		"9": ""
	}
    },
    "custom/logo": {
	"exec": "echo 'Ôüñ'",
	"format": "{}",
	"interval": 5,
	"onclick": "notify-send 'hello world'"
	
    },
    "custom/play": {
	"format": "  ÔÅã  ",
	"interval": 1,
	"on-click": "cmus-remote -p"
    },
    "custom/pause": {
	"format": "  ÔÅå  ",
	"interval": 1,
	"on-click": "cmus-remote -u"
    },
    "custom/prev": {
	"format": "  ÔÅä  ",
	"interval": 1,
	"on-click": "cmus-remote -r"
    },
    "custom/next": {
	"format": "  ÔÅé  ",
	"interval": 1,
	"on-click": "cmus-remote -n"
    },
    "custom/song": {
    	"exec": "cmus-remote -Q | grep 'tag title' | sed 's/tag title //'",
	"format": "'{}'",
	"max-length": 25,
	"interval": 1,
	"on-click": "~/.config/waybar/open-song.sh"
    },
    "custom/artist": {
    	"exec": "cmus-remote -Q | grep 'tag artist' | sed 's/tag artist //'",
	"format": "{}",
	"max-length": 16,
	"interval": 1,
	"on-click": "~/.config/waybar/open-artist.sh"
    },
    "custom/status": {
    	"exec": "cmus-remote -Q | grep 'status' | sed 's/status //'",
	"format": "{}",
	"interval": 1
    },
    "custom/clock": {
	"exec": "date +' %I:%M:%S %P '",
	"format": "{}",
	"interval": 1,
	"on-click": "flatpak run org.kde.kclock"
    }
    
}

